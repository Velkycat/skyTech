#mod_rewrite start here

RewriteEngine on

# if folder exists on server then nothing changes and the rule doesn't run

RewriteCond %{REQUEST_FILENAME} !-d

# check for file in directory with .html ext.

RewriteCond %{REQUEST_FILENAME}\.html -f

# actually shows page that has .html ext.

RewriteRule ^(.*)$ $1.html [NC,L]